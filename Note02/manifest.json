{
	"manifest_version": 2,
	"name": "永远点不到的搜索框",
	"version": "1.0",
	"description": "让你永远也点击不到淘宝搜索框",
	"content_scripts": [{
	"matches": ["*://www.taobao.com/"],
	"js": ["js/cannot_touch.js"]
	}],
	"browser_action": {
		"default_icon": {
		"19": "images/icon19.png",
		"38": "images/icon38.png"
		}
	},
	"icons": {
		"16": "images/icon16.png",
		"48": "images/icon48.png",
		"128": "images/icon128.png"
	}
}